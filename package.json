{
  "name": "kexra",
  "version": "1.1.0",
  "description": "A playful, beginner-friendly scripting language",
  "main": "dist/cli.js",
  "bin": {
    "kex": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "test": "npm run build && node dist/cli.js run examples/hello.kx && node dist/cli.js run examples/test.kx",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "language",
    "scripting",
    "toy",
    "beginner"
  ],
  "author": "manjunathh-xyz",
  "contributors": [
    "ramkrishna-xyz"
  ],
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "vite": "^5.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "prettier": "^3.0.0"
  }
}
